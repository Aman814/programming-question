#include <iostream>
using namespace std;
int main()
{
    int arr[4]={1,3,0,2};
    int old_val,n;
    n=4;
    for(int i=0;i<4;i++)
    {
        old_val=arr[i]%n;
        arr[old_val]=n*i+arr[old_val];
        
    }
    for(int i=0;i<4;i++)
    {
        
        cout<<arr[i]/n<<endl;
    }
    
}
